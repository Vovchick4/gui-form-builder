datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model Form {
    id        String @id @default(auto()) @map("_id") @db.ObjectId
    title     String
    author    String
    email     String
    formData  Json
    requested Json
}

enum EType {
    default
    checkbox
    markdown
}
